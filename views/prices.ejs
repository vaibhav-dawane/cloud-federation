    <div class="container col-md-7">
        <h1>Pricing from AWS</h1>
    </div>
    <div class="container col-md-7">
        <table class="table table-bordered border-danger">
            <thead>
              <tr>
                <th scope="col">VM Series</th>
                <th scope="col">Ram</th>
                <th scope="col">NoOfCPU</th>
                <th scope="col">Storage</th>
                <th scope="col">WindowsCost</th>
                <th scope="col">LinuxCost</th>
                <th scope="col">Action</th>
              </tr>
            </thead>
            <tbody>
                <%if(AWS!=''){%>
            
                <%for(let i = 0;i<AWS.length;i++){ %>
                <tr>
                    <form action="form" method="post">
                    <td scope="col"><%=AWS[i].Type%></td>
                    <input type="text" name="type" value="<%=AWS[i].Type%>" hidden>
                    <input type="text" name="vmos" value="<%=os%>" hidden>
                    <td scope="col"><%=AWS[i].RAM%></td>
                    <td scope="col"><%=AWS[i].noofcpu%></td>
                    <td scope="col"><%=AWS[i].Storage%></td>
                    <td scope="col"><%=AWS[i].Windowscost%></td>
                    <td scope="col"><%=AWS[i].linuxcost%></td>
                    <td scope="col"><button class="btn btn-primary">LaunchðŸš€</button></td>
                    </form>
                  </tr>
                <%}%>
          
                <%}%>

            </tbody>
          </table>
    </div>


    <div class="container col-md-7">
