    <div class="container col-md-7">
        <form action="" method="post">
            <div class="input-group mb-3">
                <span class="input-group-text" id="basic-addon1">OS</span>
                <select class="form-control" name="vmos">
                    <option value="ami-052cef05d01020f1d">Amazon Linux </option>
                    <option value="ami-0aa96960b27e44dc9">Microsoft Windows </option>
                </select>
            </div>

            <div class="input-group mb-3">
                <span class="input-group-text">Ram</span>
                <input type="text" name="ram" class="form-control" placeholder="Ram " aria-label="Recipient's username"
                    aria-describedby="basic-addon2">
                <span class="input-group-text" id="basic-addon2">GiB</span>
            </div>

            <div class="input-group mb-3">
                <span class="input-group-text" id="basic-addon3">Vcpus</span>
                <input type="text" name="vcpu" placeholder="Vcpu" class="form-control" id="basic-url"
                    aria-describedby="basic-addon3">
            </div>

            <div class="input-group mb-3">
                <span class="input-group-text">Storage</span>
                <input type="text" name="storage" placeholder="Storage" class="form-control"
                    aria-label="Amount (to the nearest dollar)">
                <span class="input-group-text">GiB</span>
            </div>

            <!-- add submit button -->
            <div class="container col-md-5">
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>
        </form>
