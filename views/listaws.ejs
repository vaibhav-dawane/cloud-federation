<div class=" container col-md-9">
    <table class="table  table-hover">
        <thead>
          <tr>
            <th scope="col">Instance No</th>
            <th scope="col">Instance ID</th>
            <th scope="col">Instance State</th>
            <th scope="col">Launch Date</th>
            <th scope="col">Instance Type</th>
            <th scope="col">Public IP</th>
            <th scope="col">Select</th>
           
          </tr>
        </thead>
        <tbody>
           <div class="container col-md-5" style="float: left;" > 
            <form action="/terminateaws" method="post">
              <input type="checkbox" name="awsterminate" value="abc" checked hidden>
              <select class="form-select" aria-label="Default select example" name="action">
                <option value="">Action</option>
                <option value="Stop">Stop</option>
                <option value="Terminate">Terminate</option>
              
              </select>
            </div>
              <%let i=1%>
            <%data.forEach(function(data1){%>
            <tr>
                <th scope="row"><%=i%></th>
                <td><%=data1.Instances[0].InstanceId%></td>
                <td><%=data1.Instances[0].State['Name']%></td>
                <td><%=data1.Instances[0].LaunchTime%></td>
                <td><%=data1.Instances[0].InstanceType%></td>
                <td><a href="http://<%=data1.Instances[0].PublicIpAddress%>" target="_blank"><%=data1.Instances[0].PublicIpAddress%></a></td>
               <td>
                  <input type="checkbox" name="awsterminate" value="<%=data1.Instances[0].InstanceId%>">
                
              </td> 
              </tr>
              <%i=i+1%>
              <%}) %>
              <div class="container col-md-2">
              <tr><button style="float:right" class="btn btn-primary ml-lg-2">Submit</button><br><br>
            </div>
            </form>
            </tbody>
      </table>
</div>
      <h1><%=data2.name%></h1>
